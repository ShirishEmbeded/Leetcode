

int trap(int* height, int heightSize)
{
   uint node[heightSize], max=height[0];
    for (int i=0; i<heightSize; i++)
    {
        node[i]= height[i];
    }
    
    for (int i=0; i<heightSize; i++)
    {
       if (max<node[i])
           max=node[i];
    }
    printf("%d\n", max);
    return 0;
}
